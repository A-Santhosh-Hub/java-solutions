import java.util.*;
class Table{
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        System.out.println("Enter the Table of Multiple number");
        int num = in.nextInt();
        System.out.println("Enter the limit of Table Number  ");
        int num1= in.nextInt();
        for(int i=1;i<=num1;i++){
            System.out.println(i+" X "+num1+" = "+i*num);
        }
    }
}
